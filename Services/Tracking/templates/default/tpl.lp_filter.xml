<fo:table width="180mm" table-layout="fixed">
	<fo:table-column column-width="60mm" />
	<fo:table-column column-width="120mm" />
	<fo:table-body>
		<fo:table-row>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm" number-columns-spanned="2" background-color="#ccdbf2">
				<fo:block font-weight="bold">{TXT_FILTER}</fo:block>
			</fo:table-cell>
		</fo:table-row>
		<!-- BEGIN filter_title -->
		<fo:table-row>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block font-weight="bold">{TXT_TITLE}</fo:block>
			</fo:table-cell>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block>{TITLE}</fo:block>
			</fo:table-cell>
		</fo:table-row>
		<!-- END filter_title -->
		<fo:table-row>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block font-weight="bold">{TXT_TYPE}</fo:block>
			</fo:table-cell>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block>{TYPE}</fo:block>
			</fo:table-cell>
		</fo:table-row>
		<fo:table-row>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block font-weight="bold">{TXT_AREA}</fo:block>
			</fo:table-cell>
			<fo:table-cell border-style="solid" border-width="0.5mm" padding="1mm">
				<fo:block>{AREA}</fo:block>
			</fo:table-cell>
		</fo:table-row>
	</fo:table-body>
</fo:table>
